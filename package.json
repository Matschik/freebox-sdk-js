{
  "name": "freebox",
  "version": "1.0.1",
  "description": "Simple authentification and secure requests to your Freebox OS server",
  "license": "MIT",
  "repository": "Matschik/freebox",
  "author": {
    "name": "Mathieu Schimmerling",
    "email": "mathieu.schimmerling@protonmail.com"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "test": "jest && xo",
    "format": "prettier --write \"**.js\""
  },
  "keywords": [
    "freebox",
    "nodejs",
    "sdk",
    "freeboxos"
  ],
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "xo": "^0.24.0"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "camelcase": 0,
      "no-else-return": 0,
      "no-async-promise-executor": 0
    },
    "overrides": [
      {
        "files": "test.js",
        "rules": {
          "no-undef": 0
        }
      }
    ]
  }
}
